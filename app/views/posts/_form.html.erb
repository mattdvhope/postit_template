<%= render 'shared/errors', obj: @post %>

<%= form_for @post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br/>

  <%= f.label :url %>
  <%= f.text_field :url %>
  <br/>

  <%= f.label :description %>
  <%= f.text_field :description %>
  <br/>

  <%= f.label :user_id %>
  <%= f.collection_select :user_id, @user_count, :id, :username %>
  <br/>

  <%= f.submit "Create Post", class: 'btn btn-primary' %>
<% end %>